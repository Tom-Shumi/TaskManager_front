{"ast":null,"code":"var conversionStatus = function conversionStatus(status) {\n  var str;\n\n  switch (status) {\n    case 1:\n      str = 'Not Started';\n      break;\n\n    case 2:\n      str = 'In Progress';\n      break;\n\n    case 3:\n      str = 'Done';\n      break;\n  }\n\n  return str;\n};\n\nvar conversionStatusByTime = function conversionStatusByTime(quota, doneTime, deleteFlg) {\n  if (deleteFlg == 1) {\n    return {\n      str: \"DELETE\",\n      color: \"gray\"\n    };\n  }\n\n  if (doneTime == 0 || doneTime == null) {\n    return {\n      str: conversionStatus(1),\n      color: \"yellow\"\n    };\n  } else if (quota > doneTime) {\n    return {\n      str: conversionStatus(2),\n      color: \"white\"\n    };\n  } else {\n    return {\n      str: conversionStatus(3),\n      color: \"gray\"\n    };\n  }\n};\n\nvar conversionPriority = function conversionPriority(priority) {\n  var str;\n  var className;\n\n  switch (priority) {\n    case 1:\n      str = 'LOW';\n      className = 'blue';\n      break;\n\n    case 2:\n      str = 'MEDIUM';\n      className = 'green';\n      break;\n\n    case 3:\n      str = 'HIGH';\n      className = 'red';\n      break;\n  }\n\n  return {\n    str: str,\n    className: className\n  };\n};\n\nvar conversionDateStr = function conversionDateStr(status) {\n  // 日付表示文字列設定\n  var dateStr;\n\n  switch (status) {\n    case 1:\n    case 2:\n      dateStr = 'Plan Date';\n      break;\n\n    case 3:\n      dateStr = 'Done Date';\n      break;\n  }\n\n  return dateStr;\n};\n\nexport { conversionStatus, conversionPriority, conversionDateStr, conversionStatusByTime };","map":{"version":3,"sources":["/Users/shumiya/Desktop/dev/react/shumipro_front/src/components/util/ConversionUtil.ts"],"names":["conversionStatus","status","str","conversionStatusByTime","quota","doneTime","deleteFlg","color","conversionPriority","priority","className","conversionDateStr","dateStr"],"mappings":"AACA,IAAMA,gBAAgB,GAAG,SAAnBA,gBAAmB,CAACC,MAAD,EAAoB;AAC