{"ast":null,"code":"export var convertHourMinute = function convertHourMinute(time) {\n  if (time == null) {\n    return '0 m';\n  }\n\n  var hour = Math.floor(time / 60);\n  var minute = time % 60;\n  var hourMinute = \"\";\n\n  if (hour < 0) {\n    return \"0 m\";\n  }\n\n  if (hour != 0) {\n    hourMinute += \"\".concat(hour, \" h\");\n  }\n\n  if (minute != 0) {\n    hourMinute = hourMinute == \"\" ? hourMinute : hourMinute + \" \";\n    hourMinute += \"\".concat(minute, \" m\");\n  }\n\n  return hourMinute == \"\" ? \"0 m\" : hourMinute;\n};\nexport var convertRemaining = function convertRemaining(quota, doneTime) {\n  if (doneTime == null) {\n    return convertHourMinute(quota);\n  } else {\n    return convertHourMinute(quota - doneTime);\n  }\n};\nexport var isNumber = function isNumber(str) {\n  // チェック条件パターン\n  var pattern = /^[+,-]?([1-9]\\d*|0)$/; // 数値チェック\n\n  return pattern.test(str);\n};","map":null,"metadata":{},"sourceType":"module"}